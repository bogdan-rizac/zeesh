# freebsd / zeesh! plugin

export LSCOLORS=ExfxcxdxbxegedabagAcEx
export PATH=~/.bin:~/.bin/ellipsis:/usr/local/bin:/usr/local/sbin:/usr/bin/:/usr/sbin:/bin:/sbin

alias ls='ls -AGF'
compctl -K list_sysctls sysctl
compctl -c man
compctl -c info
compctl -c which
# no core dumps
limit core 0

function ps() {
    if [ $1 ]; then
        /bin/ps $@
    else
        /bin/ps ux
    fi
}

function last-modified() {
    stat -f '%m' $1
}
