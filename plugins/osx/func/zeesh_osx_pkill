# kill matching processes
zeesh_osx_pkill () {
    if [ $1 ]; then
        pids=($(zeesh_psgrep "$@" | tr -s '[:space:]' | cut -d ' ' -f2 ))
        if [[ $pids[1] != '' ]]; then
            echo "killed:"
            zeesh_psgrep "$@"
            for pid in $pids
            do
                /bin/kill -9 $pid
            done
        else
            echo "No matches"
        fi
        unset pids
    fi
}
