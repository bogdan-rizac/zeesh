# plugins to enable (order can be important)
zeesh_plugins=(
    #color_switch        # lets you adjust ansi colors
    #login_info          # displays login info
    #haskell
    python              # python funcs/aliases
    virtualenv          # virtualenv wrapper
    mercurial           # mercurial func/aliases
    django              # django func/aliases
    #pianobar
    virtualbox
    #upgrade             # automatic version check and upgrade
    vcs_info
    syntax-highlighting
    history-substring-search
    theme
)
source ~/.zsh/zeesh.zsh
source ~/.zsh/local/zshrc
